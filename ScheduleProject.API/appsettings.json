{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "MSSQL": "YOUR_CONNECTION_STRING"
  },

  "TelegramMiniAppAuthorizationOptions": {
    "Token": "YOUR_BOT_TOKEN",
    "AuthDataValidInterval": "01:00:00"
  },

  "JwtOptions": {
    "SecretKey": "YOUR_SECRET_KEY",
    "Audience": "Audience",
    "Issuer": "Issuer",
    "AccessTokenCookieKey": "access",
    "RefreshTokenCookieKey": "refresh",
    "AccessTokenExpiryMinutes": "20",
    "RefreshTokenExpiryDays": "30"
  }
}
